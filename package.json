    "url": "git+ssh://git@github.com/insaic/neon.git"
  },
  "bugs": {
    "url": "https://github.com/insaic/neon/issues"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "last 3 versions",
    "ie >= 10",
    "ie_mob >= 10",
    "ff >= 30",
    "chrome >= 34",
    "safari >= 6",
    "opera >= 12.1",
    "ios >= 7",
    "android >= 4.1",
    "bb >= 10",
    "and_uc 9.9"
  ],
  "homepage": "https://github.com/insaic/neon#readme",
